{
  "name": "touched-agi",
  "version": "0.0.1",
  "description": "A fork and modified version of ts-agi which will see if the Asterisk channel is still open, or if the caller dropped the call. This will resolve Auto-Destruct issue on ts-agi.",
  "main": "index.ts",
  "scripts": {
    "dev": "ts-node ./index.ts",
    "build": "yarn tsc",
    "start": "node dist/index.js"
  },
  "keywords": [
    "asterisk",
    "agi",
    "typescript",
    "fastagi"
  ],
  "author": "Sylvester Stephenson",
  "license": "ISC",
  "dependencies": {
    "types": "^0.1.1",
    "typescript": "^4.2.4",
    "debug": "^4.1.1"
  },
  "devDependencies": {
    "memorystream": "^0.3.1",
    "@types/memorystream": "^0.3.0",
    "@types/debug": "^4.1.5",
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.6.3"
  }
}
